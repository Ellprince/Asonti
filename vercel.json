{
  "buildCommand": "cd \"Asonti app\" && npm install --legacy-peer-deps && npm run build",
  "outputDirectory": "Asonti app/build",
  "installCommand": "cd \"Asonti app\" && npm install --legacy-peer-deps",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}